<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

<module package="com.axelor.gst.db" name="gst"/>
<entity name="Invoice" cacheable="true" >
<string name="status"  readonly="true"/>
<many-to-one name="company" ref="Company" required="true" default="Company"></many-to-one>
<string name="reference" />
<datetime name="dates" />
<integer name="priority"/>
<many-to-one name="party" ref="Party" required="true"/>
<many-to-one name="party_contact" ref="Contact"/>
<many-to-one name="invoice_address" ref="Address"  required="true" ></many-to-one>
<many-to-one name="shipping_address" ref="Address" />
<boolean name="use_invoice_address"/>
<decimal name="net_amount" readonly="true" />
<decimal name="net_IGST" readonly="true"/>
<decimal name="net_CSGT" readonly="true"/>
<decimal name="net_SGST" readonly="true"/>
<decimal name="gross_amount"/>
</entity>
  
  
  </domain-models>