<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

<module package="com.axelor.gst.db" name="gst"/>
<entity name="Invoice" cacheable="true" >
<string name="status" readonly="true" selection="status-selections"/>
<many-to-one name="companies" ref="Company" required="true" default="company"/>
<string name="reference" readonly="true"/>
<datetime name="dates" />
<integer name="priority"/>
<many-to-one name="partys" ref="Party" required="true"/>
<many-to-one name="party_contact" ref="Contact" required="true"/>
<many-to-one name="invoice_address" ref="Address"/>
<many-to-one name="shipping_address" ref="Address"/>
<boolean name="use_invoice_address" />
<decimal name="net_amount" readonly="true" hidden="false"/>
<decimal name="net_IGST" readonly="true" hidden="false"/>
<decimal name="net_CSGT" readonly="true" hidden="false"/>
<decimal name="net_SGST" readonly="true" hidden="false"/>
<decimal name="gross_amount" readonly="true" hidden="false"/>
<one-to-many name="invoice_items" ref="Invoice_line"/>

</entity>

  
  
  </domain-models>