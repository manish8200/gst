<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">
  
  <module package="com.axelor.gst.db" name="gst"/>
  <entity name="City" cacheable="true">
  <string name="name" namecolumn="true" required="true"/>
  <many-to-one name="state"	 ref="State" required="true"/>
  <many-to-one name="country" ref="Country" required="true"/>
  </entity>
  
  </domain-models>