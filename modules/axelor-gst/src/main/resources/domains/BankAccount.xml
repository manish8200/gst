<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

	<module package="com.axelor.gst.db" name="gst" />
	<entity name="BankAccount" cacheable="true">
		<string name="bank" required="true" />
		<string name="accountNo" namecolumn="true" required="true" />
		<string name="IFSC" required="true" />
		<string name="type" default="current" />
		<many-to-one name="address" ref="Address" />
	</entity>


</domain-models>