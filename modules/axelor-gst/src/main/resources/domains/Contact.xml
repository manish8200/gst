<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

	<module package="com.axelor.gst.db" name="gst" />
	<entity name="Contact">
		<string name="name" title="Contact Name" required="true" />
		<string name="type" title="Contact Type" required="true" />
		<string name="primaryEmail" title="Contact Primary Email" />
		<string name="secondaryEmail" title="Contact Secondary Email" />
		<string name="primaryPhone" title="Contact Primary Phone number" />
		<string name="secondaryPhone" title="Contact Secondary number"/>
<many-to-one name="party" title="Party Contact" ref="com.axelor.gst.db.Party"/>

	</entity>


</domain-models>