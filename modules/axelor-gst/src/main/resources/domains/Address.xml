<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

	<module package="com.axelor.gst.db" name="gst" />
	<entity name="Address" cacheable="true">
		<string name="type" required="true" />
		<string name="fullAddresss" namecolumn="true" />
		<string name="line1" required="true" />
		<string name="line2" />
		<many-to-one name="city" ref="City" required="true" />
		<many-to-one name="state" ref="State" />
		<many-to-one name="country" ref="Country" required="true" />
		<string name="pincode" />


	</entity>


</domain-models>